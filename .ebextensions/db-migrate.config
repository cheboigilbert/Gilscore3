container_commands:

  
  01_makemigrations:
    command: "django-admin manage.py makemigrations --noinput "
    leader_only: true

  02_migrate:
    command: "django-admin manage.py migrate "
    leader_only: true


  03_createsuperuser:
      command: "django-admin manage.py createsuperuser "
      leader_only: true
  04_collectstatic:
    command: "django-admin  manage.py collectstatic  "



option_settings:
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: tweetme2.settings